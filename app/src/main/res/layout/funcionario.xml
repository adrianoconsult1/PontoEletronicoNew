<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Swipe"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:context=".FuncListActivity">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" tools:layout_editor_absoluteY="55dp" tools:layout_editor_absoluteX="368dp"
        tools:ignore="MissingConstraints"
        android:background="@drawable/bghex2">
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:context=".FuncListActivity">
        <androidx.appcompat.widget.Toolbar
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:background="@color/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:layout_marginTop="0.0000000000001dp"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/barraMain"
            tools:ignore="MissingConstraints" />
        <ExpandableListView

            android:id="@+id/lvExp"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@drawable/bghex2"
            android:layout_marginTop="50dp"
            android:gravity="left"
            android:dividerHeight="10dp" tools:layout_editor_absoluteY="41dp"
            tools:layout_editor_absoluteX="0dp"
            tools:ignore="MissingConstraints">
        </ExpandableListView>
    </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>